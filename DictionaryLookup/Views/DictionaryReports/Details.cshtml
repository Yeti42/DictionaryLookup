@model DictionaryLookup.Models.DictionaryReport

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>DictionaryReport</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Word
        </dt>

        <dd>
            @Html.DisplayFor(model => model.WordID)
        </dd>

        <dt>
            Language
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LanguageID)
        </dd>

        <dt>
            Date/Time
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ReportDateTime)
        </dd>

        <dt>
            Error Reported
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ErrorTypeID)
        </dd>

        <dt>
            Username
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserID)
        </dd>

        <dt>
            Comments
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Notes)
        </dd>

    </dl>
</div>